@model List<Mitovia.Models.MeasureScale>;

<h1>MeasureScale List</h1>

@{

    if (Model.Count == 0)
    {
        <p>No Measure found</p>
    }
    else
    {
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">OrgID</th>
                    <th scope="col">Name</th>
                    <th scope="col">DisplayName</th>
                    <th scope="col">Description</th>
                    <th scope="col">LastUpdatedDate</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var measureScales in Model)
                {
                    <tr>
                        <td>@measureScales.ID</td>
                        <td>@measureScales.OrgID</td>
                        <td>@measureScales.Name</td>
                        <td>@measureScales.DisplayName</td>
                        <td>@measureScales.Description</td>
                        <td>@measureScales.LastUpdatedDate</td>
                    </tr>
                }
            </tbody>
        </table>
    }
}